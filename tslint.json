{
  "defaultSeverity": "error",
  "extends": ["tslint-config-airbnb"],
  "jsRules": {},
  "rules": {
    "jsx-wrap-multiline": false,
    "align": false,
    "variable-name": false,
    "import-name": [false, {}],
    "trailing-comma": true,
    "ter-arrow-parens": [
      false,
      "as-needed",
      {
        "requireForBlockBody": true
      }
    ]
  },
  "rulesDirectory": []
}
